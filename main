List=[]
n=int(input("Enter the number of elements that you want to add in the list: "))
for i in range(n):
    value=int(input("Enter the value to be inserted in list: "))
    List.append(value)
def sort(List):
    for i in range(n):
        for j in range(n-i-1):
            if List[j]>List[j+1]:
                temp=List[j]
                List[j]=List[j+1]
                List[j+1]=temp

print("Sorted List is:")
print(sort(List))
